<div class='span10'>

  <%= render 'products/breadcrumb' %>

  <% products.in_groups_of(3) do |group| %>
    <table>
      <tr>
        <% group.each do |product| %>
          <td>
            <% if product.blank? %>
              &nbsp;
            <% else %>
              <%= render 'products/product', product: product %>
            <% end %>
          </td>
        <% end %>
      </tr>
    </table>
  <% end %>
</div>
